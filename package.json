{
  "name": "deel-task",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@sequelize/core": "^7.0.0-alpha.10",
    "body-parser": "^1.19.0",
    "express": "4.17.1",
    "express-async-handler": "^1.2.0",
    "sequelize": "^6.3.4",
    "sqlite3": "^4.0.1"
  },
  "scripts": {
    "start": "nodemon ./src/server.js",
    "seed": "node ./scripts/seedDb.js"
  },
  "devDependencies": {
    "concurrently": "4.1.0",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "nodemon": "1.19.1"
  },
  "engines": {
    "node": ">=10.16.3"
  },
  "engineStrict": true,
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
        "es6": true,
        "browser": true
    },
    "rules": {
        "max-len": ["error", { "code": 120 }],
        "brace-style": [
            "error",
            "stroustrup"
        ],
        "comma-dangle": [
            "error",
            "never"
        ],
        "no-unused-vars": [
            "warn"
        ],
        "no-var": [
            "off"
        ],
        "one-var": [
            "off"
        ]
    }
  }
}
